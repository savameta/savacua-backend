    "nanoid": "^5.0.5",
    "nestjs-prisma": "^0.23.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "prisma-dbml-generator": "^0.10.0",
    "reflect-metadata": "^0.2.1",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.4",
    "swagger-ui-express": "^5.0.0",
    "urlcat": "^3.1.0"
